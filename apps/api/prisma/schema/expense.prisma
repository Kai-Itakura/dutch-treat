model Expense {
  id     String @id
  title  String
  amount Int

  payerId String
  payer   User   @relation(fields: [payerId], references: [id], name: "payer")

  payees User[] @relation(name: "payee")

  groupId String
  group   EventGroup @relation(fields: [groupId], references: [id], onDelete: Cascade)
}
